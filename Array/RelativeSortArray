class Solution {
 public:
  vector<int> relativeSortArray(vector<int>& arr1, vector<int>& arr2) {
    vector<int> a;
    vector<int> c(1001);

    for (int a : arr1)
      ++c[a];

    for (int a : arr2)
      while (c[a]-- > 0)
        a.push_back(a);

    for (int num = 0; num < 1001; ++num)
      while (c[num]-- > 0)
        a.push_back(num);

    return a;
  }
};//1122
